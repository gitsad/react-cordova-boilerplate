    "memory-fs": "^0.4.1",
    "mocha": "^3.2.0",
    "mocha-webpack": "next",
    "node-sass": "^4.3.0",
    "nyc": "^10.2.0",
    "prop-types": "^15.5.8",
    "radium": "^0.18.1",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "require-from-string": "^1.2.0",
